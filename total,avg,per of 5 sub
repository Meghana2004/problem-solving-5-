#include<stdio.h>
int main()
{
    float total,avg,percentage;
    int s1,s2,s3,s4,s5;
    printf("enter the marks of 5 subjects");
    scanf("%d%d%d%d%d",&s1,&s2,&s3,&s4,&s5);
    total=s1+s2+s3+s4+s5;
    avg=total/5;
    percentage=(total/500)*100;
    printf("total marks of 5 subjects is %f\navg of 5 subjects is %f\npercenatge of 5 subjects is %f",total,avg,percentage);
}
